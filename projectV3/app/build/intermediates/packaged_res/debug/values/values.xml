        <item name="colorPrimary">@color/xiangzhang_primary</item>
        <item name="colorPrimaryVariant">@color/xiangzhang_primary_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/xiangzhang_secondary</item>
        <item name="colorSecondaryVariant">@color/xiangzhang_secondary_dark</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor">@color/xiangzhang_primary_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:navigationBarColor">@color/background_default</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="Theme.ProjectV3.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
